{"ast":null,"code":"import { ref, defineComponent, nextTick } from 'vue';\nimport fakeShopApi from '../api/backend_call';\nexport default defineComponent({\n  setup() {\n    const name = ref(null);\n    const email = ref(null);\n    const avatar = ref(null);\n    const role = ref(null);\n    async function fetchData() {\n      let token = localStorage.getItem(\"access_token\");\n      const resp = await backend_call.get('auth/profile', {\n        headers: {\n          Authorization: `Bearer ${token}`\n        }\n      });\n      nextTick(() => {\n        name.value = resp.data.name;\n        email.value = resp.data.email;\n        avatar.value = resp.data.avatar;\n        role.value = resp.data.role;\n      });\n    }\n    fetchData();\n    return {\n      name,\n      email,\n      avatar,\n      role\n    };\n  }\n});","map":{"version":3,"mappings":"AAUA,SAASA,GAAG,EAAEC,eAAe,EAAEC,QAAO,QAAS,KAAK;AACpD,OAAOC,WAAU,MAAO,qBAAqB;AAE7C,eAAeF,eAAe,CAAC;EAC7BG,KAAK,GAAG;IACN,MAAMC,IAAG,GAAIL,GAAG,CAAC,IAAI,CAAC;IACtB,MAAMM,KAAI,GAAIN,GAAG,CAAC,IAAI,CAAC;IACvB,MAAMO,MAAK,GAAIP,GAAG,CAAC,IAAI,CAAC;IACxB,MAAMQ,IAAG,GAAIR,GAAG,CAAC,IAAI,CAAC;IAEtB,eAAeS,SAAS,GAAG;MACzB,IAAIC,KAAI,GAAIC,YAAY,CAACC,OAAO,CAAC,cAAc,CAAC;MAChD,MAAMC,IAAG,GAAI,MAAMC,YAAY,CAACC,GAAG,CAAC,cAAc,EAAE;QAACC,OAAO,EAAE;UAACC,aAAa,EAAG,UAASP,KAAM;QAAC;MAAC,CAAC,CAAC;MAClGR,QAAQ,CAAC,MAAM;QACbG,IAAI,CAACa,KAAI,GAAIL,IAAI,CAACM,IAAI,CAACd,IAAI;QAC3BC,KAAK,CAACY,KAAI,GAAIL,IAAI,CAACM,IAAI,CAACb,KAAK;QAC7BC,MAAM,CAACW,KAAI,GAAIL,IAAI,CAACM,IAAI,CAACZ,MAAM;QAC/BC,IAAI,CAACU,KAAI,GAAIL,IAAI,CAACM,IAAI,CAACX,IAAI;MAC7B,CAAC,CAAC;IACJ;IAEAC,SAAS,EAAE;IAEX,OAAO;MACLJ,IAAI;MACJC,KAAK;MACLC,MAAM;MACNC;IACF,CAAC;EACH;AACF,CAAC,CAAC","names":["ref","defineComponent","nextTick","fakeShopApi","setup","name","email","avatar","role","fetchData","token","localStorage","getItem","resp","backend_call","get","headers","Authorization","value","data"],"sourceRoot":"","sources":["/Users/emilyvelzeboer/Documents/Bootcamp/WebDevelopment/flitter/frontend/src/views/AboutView.vue"],"sourcesContent":["<template>\n  <div>\n    <h1>{{ name }}</h1>\n    <p>{{ email }}</p>\n    <img :src=\"avatar\" alt=\"\">\n    <p>Role: {{ role }}</p>\n  </div>\n</template>\n\n<script>\nimport { ref, defineComponent, nextTick } from 'vue';\nimport fakeShopApi from '../api/backend_call';\n\nexport default defineComponent({\n  setup() {\n    const name = ref(null);\n    const email = ref(null);\n    const avatar = ref(null);\n    const role = ref(null);\n\n    async function fetchData() {\n      let token = localStorage.getItem(\"access_token\");\n      const resp = await backend_call.get('auth/profile', {headers: {Authorization: `Bearer ${token}`}});\n      nextTick(() => {\n        name.value = resp.data.name;\n        email.value = resp.data.email;\n        avatar.value = resp.data.avatar;\n        role.value = resp.data.role;\n      });\n    }\n\n    fetchData();\n\n    return {\n      name,\n      email,\n      avatar,\n      role\n    };\n  }\n});\n</script>\n\n\n<style scoped>\ndiv{\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  flex-direction: column;\n}\n\nimg{\n  width: 200px;\n  border: 2px solid black;\n  border-radius: 30px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}